<%- include("svgs") %>
  <header
    class="vh100 overflow-hidden d-flex flex-wrap justify-content-center py-3 mb-4 border-bottom">
    <nav class="navbar nav-main-color fixed-top">
      <div class="container-fluid">
        <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto link-body-emphasis text-decoration-none">
          <!-- <svg class="bi me-2" width="40" height="32"><use xlink:href="#bootstrap"></use></svg>  -->
          <span class="fs-4">&nbsp;Robo Rental <p style="display:inline; position: relative; bottom:1px">|</p><img
              style="position: relative; bottom:3px;" width="34px;" src="robot.png">&nbsp; &nbsp;&nbsp;&nbsp;</span>
        </a>

        <!-- Check if at the index page or not (variable atIndexPage passed in in '/' route handler) -->
        <% if (atIndexPage == false) { %>
        <!-- If not at index page, don't load the navbar options -->

        <button class="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar"
          aria-controls="offcanvasNavbar" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="offcanvas offcanvas-end" tabindex="-1" id="offcanvasNavbar" aria-labelledby="offcanvasNavbarLabel">
          <div class="offcanvas-header">
            <h5 class="offcanvas-title" id="offcanvasNavbarLabel"><span class="fs-4">&nbsp;Robo Rental <p
                  style="display:inline; position: relative; bottom:1px">|</p><img
                  style="position: relative; bottom:3px;" width="34px;" src="robot.png">&nbsp; &nbsp;&nbsp;&nbsp;</span>
            </h5>
            <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
          </div>
          <div class="offcanvas-body">
            <ul class="navbar-nav justify-content-end flex-grow-1 pe-3">
              <% for(var i=0; i < locals.navLinks.length; i++) { %>
                <li class="nav-item">
                  <a href="<%= locals.navLinks[i].link %>" style="color: black;"
                    class="nav-link <% if (locals.navLinks[i].link == currentURL)  { %> active <% }%>"
                    aria-current="page" onmouseover="this.style.color='silver'" onmouseout="this.style.color='black'">
                    <%= locals.navLinks[i].name %>
                  </a>
                </li>
                <% } %>
            </ul>
          </div>
        </div>

        <!-- If at the index page, load signup and login buttons -->
        <% } else { %>
          <!-- <div id="index-login-signup-button-container">
            <button type='button' onclick="window.location.href = '/login';" class="btn btn-light btn-outline-secondary py-2 rounded-pill index-signup-login-buttons">Log in</button>
            <button type='button' onclick="window.location.href = '/signup';" class="btn py-2 rounded-pill index-signup-login-buttons" style="background-color: #003249 !important; color: white;">Sign up</button>
          </div> -->
        <% } %>

      </div>
    </nav>
  </header>

  <body>

    <div id="content-and-footer-flex-container">
      <!-- Start of flexbox container to make the main content container fill all of the space after header and before footer -->
      <div id="all-pages-content-container"> <!-- Start of main content -->